{
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true,
    "vueIndentScriptAndStyle": false,
    "printWidth": 250,
    "bracketSameLine": false,
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.mts", "*.d.ts"],
            "options": {
                "parser": "typescript"
            }
        }
    ]
}
